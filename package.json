{
    "name": "crypto_currency_viz_pred",
    "version": "0.0.1",
    "license": "MIT",
    "description": "Crypto Currency Visualization and Prediction",
    "engines": {
        "node": "16.x"
    },
    "scripts": {
        "start": "next start",
        "build": "next build",
        "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf .next/ && rm -rf out/ && npm install && npm run dev",
        "compile:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js assets/scss/nextjs-argon-dashboard.scss assets/css/nextjs-argon-dashboard.css",
        "minify:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js assets/scss/nextjs-argon-dashboard.scss assets/css/nextjs-argon-dashboard.min.css --output-style compressed",
        "map:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js assets/scss/nextjs-argon-dashboard.scss assets/css/nextjs-argon-dashboard.css --source-map true",
        "build:scss": "npm run compile:scss && npm run minify:scss && npm run map:scss",
        "dev": "next dev",
        "start:local": "PORT=4000 node ./server-local.js",
        "dev:ssl": "next dev -p 3002 & local-ssl-proxy --source 3000 --target 3002",
        "start-ssl": "node server.js"
    },
    "dependencies": {
        "@faker-js/faker": "^7.6.0",
        "@fortawesome/fontawesome-free": "^5.15.2",
        "axios": "^0.21.4",
        "bcrypt": "^5.0.1",
        "bootstrap": "^4.6.0",
        "chart.js": "^4.2.1",
        "classnames": "^2.2.6",
        "handlebars": "^4.7.7",
        "js-cookie": "^3.0.1",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.1",
        "next": "11.0.1",
        "next-redux-wrapper": "^7.0.5",
        "nodemailer": "^6.7.0",
        "nookies": "^2.5.2",
        "nouislider": "^14.6.3",
        "nprogress": "^0.2.0",
        "react": "^17.0.2",
        "react-bootstrap-typeahead": "^6.0.0-alpha.4",
        "react-chartjs-2": "^5.2.0",
        "react-copy-to-clipboard": "^5.0.3",
        "react-datetime": "^3.0.4",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.6",
        "reactstrap": "^8.9.0",
        "redux": "4.1.2",
        "redux-logger": "^3.0.6",
        "redux-persist": "^6.0.0",
        "redux-saga": "^1.1.3",
        "redux-storage": "4.1.2",
        "sass": "^1.39.2",
        "shortid": "^2.2.16",
        "validator": "^13.6.0",
        "webpack": "^5.52.1"
    }
}
